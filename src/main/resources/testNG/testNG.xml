<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ParallelExecution" parallel="tests" thread-count="3">
    <test name="1024x768">
        <parameter name="resolution" value="1024,768"/>
        <classes>
            <class name="tests.SearchTest"/>
        </classes>
    </test>
    <test name="1440x900">
        <parameter name="resolution" value="1440,900"/>
        <classes>
            <class name="tests.SearchTest"/>
        </classes>
    </test>
    <test name="1366x768">
        <parameter name="resolution" value="1366,768"/>
        <classes>
            <class name="tests.SearchTest"/>
        </classes>
    </test>
</suite>